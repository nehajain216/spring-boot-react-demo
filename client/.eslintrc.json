{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-undef": "off",
    "no-unused-vars": [
      "off",
      {
        "args": "none"
      }
    ],
    "react/prop-types": "off",
    "react/jsx-key": "off",
    "no-empty": "error",
    "no-extra-semi": "error"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}